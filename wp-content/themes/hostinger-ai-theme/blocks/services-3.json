  "content": "<!-- wp:group {\"align\":\"full\",\"className\":\"hostinger-ai-about-us-3\",\"layout\":{\"type\":\"constrained\"}} -->\n<div class=\"wp-block-group alignfull hostinger-ai-about-us-3 hostinger-ai-fade-up\"><!-- wp:columns {\"verticalAlignment\":null,\"align\":\"wide\"} -->\n    <div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":\"60%\"} -->\n        <div class=\"wp-block-column\" style=\"flex-basis:60%\"><!-- wp:image {\"id\":542,\"sizeSlug\":\"full\",\"linkDestination\":\"none\",\"className\":\"hostinger-ai-service-image hostinger-index-3\",\"style\":{\"border\":{\"radius\":\"20px\"}}} -->\n            <figure class=\"wp-block-image size-full has-custom-border hostinger-ai-service-image hostinger-index-3\"><img src=\"https://hostinger-wp-blocks.com/wp-content/uploads/2024/11/700x500.png\" alt=\"\" class=\"wp-image-542\" style=\"border-radius:20px\"/></figure>\n            <!-- /wp:image --></div>\n        <!-- /wp:column -->\n\n        <!-- wp:column {\"verticalAlignment\":\"center\",\"width\":\"40%\"} -->\n        <div class=\"wp-block-column is-vertically-aligned-center\" style=\"flex-basis:40%\"><!-- wp:group {\"style\":{\"spacing\":{\"blockGap\":\"0px\",\"padding\":{\"right\":\"var:preset|spacing|20\",\"left\":\"var:preset|spacing|20\"}},\"layout\":{\"columnSpan\":1,\"rowSpan\":1}},\"layout\":{\"type\":\"flex\",\"orientation\":\"vertical\",\"justifyContent\":\"left\",\"flexWrap\":\"nowrap\",\"verticalAlignment\":\"center\"}} -->\n            <div class=\"wp-block-group hostinger-ai-fade-up\" style=\"padding-right:var(--wp--preset--spacing--20);padding-left:var(--wp--preset--spacing--20)\"><!-- wp:heading {\"className\":\"hostinger-ai-service-title\",\"style\":{\"spacing\":{\"margin\":{\"top\":\"0\",\"bottom\":\"0\",\"left\":\"0\",\"right\":\"0\"},\"padding\":{\"bottom\":\"var:preset|spacing|20\"}},\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|dark\"}}}},\"textColor\":\"dark\",\"fontSize\":\"x-large\"} -->\n        