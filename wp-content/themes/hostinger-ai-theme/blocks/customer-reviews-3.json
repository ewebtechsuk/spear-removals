ragraph {\"className\":\"hostinger-ai-testimonial-text\",\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|dark\"}}},\"layout\":{\"columnSpan\":1,\"rowSpan\":1},\"spacing\":{\"padding\":{\"left\":\"var:preset|spacing|20\"}}},\"textColor\":\"dark\",\"fontSize\":\"medium\"} -->\n                        <p class=\"hostinger-ai-testimonial-text has-dark-color has-text-color has-link-color has-medium-font-size\" style=\"padding-left:var(--wp--preset--spacing--20)\">Amazing stay at Skialps! The accommodation was perfect for our ski trip.</p>\n                        <!-- /wp:paragraph --></div>\n                    <!-- /wp:group --></div>\n                <!-- /wp:group --></div>\n            <!-- /wp:column --></div>\n        <!-- /wp:columns --></div>\n    <!-- /wp:group --></div>\n<!-- /wp:group -->",
