  "content": "<!-- wp:group {\"align\":\"wide\",\"className\":\"hostinger-ai-fade-up hostinger-ai-customer-reviews-3\",\"layout\":{\"type\":\"flex\",\"flexWrap\":\"nowrap\",\"justifyContent\":\"center\"}} -->\n<div class=\"wp-block-group alignwide hostinger-ai-fade-up hostinger-ai-customer-reviews-3\"><!-- wp:group {\"align\":\"wide\",\"className\":\"hostinger-ai-fade-up\",\"layout\":{\"type\":\"default\"}} -->\n    <div class=\"wp-block-group alignwide hostinger-ai-fade-up\"><!-- wp:columns -->\n        <div class=\"wp-block-columns\"><!-- wp:column {\"width\":\"40%\"} -->\n            <div class=\"wp-block-column\" style=\"flex-basis:40%\"><!-- wp:heading {\"textAlign\":\"left\",\"align\":\"full\",\"className\":\"hostinger-ai-subtitle\",\"style\":{\"elements\":{\"link\":{\"color\":{\"text\":\"var:preset|color|dark\"}}},\"spacing\":{\"margin\":{\"bottom\":\"var:preset|spacing|50\"}}},\"textColor\":\"dark\",\"fontSize\":\"x-large\"} -->\n                <h2 class=\"wp-block-heading alignfull has-text-align-left hostinger-ai-subtitle has-dark-color has-text-color has-link-color has-x-large-font-size\" style=\"margin-bottom:var(--wp--preset--spacing--50)\">Customer reviews</h2>\n                <!-- /wp:heading --></div>\n            <!-- /wp:column -->\n\n            <!-- wp:column {\"width\":\"60%\"} -->\n            <div class=\"wp-block-column\" style=\"flex-basis:60%\"><!-- wp:group {\"layout\":{\"type\":\"flex\",\"orientation\":\"vertical\"}} -->\n                <div class=\"wp-block-group\"><!-- wp:group {\"className\":\"hostinger-ai-fade-up\",\"style\":{\"spacing\":{\"padding\":{\"bottom\":\"var:preset|spacing|40\"}}},\"layout\":{\"type\":\"flex\",\"flexWrap\":\"nowrap\"}} -->\n                    <div class=\"wp-block-group hostinger-ai-fade-up\" style=\"padding-bottom:var(--wp--preset--spacing--40)\"><!-- wp:image {\"id\":266,\"scale\":\"cover\",\"sizeSlug\":\"full\",\"linkDestination\":\"none\",\"className\":\"hostinger-ai-testimonial-image hostinger-index